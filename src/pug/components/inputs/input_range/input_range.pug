mixin input_range(className, title1, title2, placeholder1="ДД.ММ.ГГГГ", placeholder2="ДД.ММ.ГГГГ")
    div(class='input-range range-'+className)
        +input_dropdown([className, ''], placeholder1, title1)
        +input_dropdown([className, ''], placeholder2, title2)
        .input-range__calendar