
include ../../../components/buttons/btn_type_one/btn_type_one.pug

mixin header(className, logined=false)
    header.header.container 
        .header__logo 
            img.header__logo(src=require('../../../assets/img/logo.png'), alt="")
        .header__nav
            +btn_type_one('header__nav-btn', 'header button')
            +btn_type_one('header__nav-btn', 'header button')
